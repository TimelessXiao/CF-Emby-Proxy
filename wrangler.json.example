{
  "$schema": "./node_modules/wrangler/config-schema.json",
  "name": "cf-emby-proxy",
  "main": "src/worker.js",
  "compatibility_date": "2025-12-28",
  "compatibility_flags": [
    "nodejs_compat"
  ],
  "kv_namespaces": [
    {
      "binding": "ROUTE_MAP",
      "id": "REPLACE_WITH_YOUR_KV_NAMESPACE_ID",
      "preview_id": "REPLACE_WITH_YOUR_PREVIEW_NAMESPACE_ID"
    }
  ],
  "vars": {
    "UPSTREAM_URL": "https://your-emby-server.com",
    "DEBUG": "false"
  },
  "observability": {
    "enabled": false,
    "head_sampling_rate": 1,
    "logs": {
      "enabled": false,
      "head_sampling_rate": 1,
      "persist": false,
      "invocation_logs": false
    },
    "traces": {
      "enabled": false,
      "persist": true,
      "head_sampling_rate": 1
    }
  }
}
